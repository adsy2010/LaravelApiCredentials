{
    "name": "adsy2010/laravel-api-credentials",
    "description": "This package is designed to provide an encrypted layer to API keys so they are not left exposed in env or config files directly",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "adsy2010",
            "email": "adsy2010@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5 | ^7.0",
        "illuminate/support": "5.8.* | ^6.0 | ^7.0 | ^8.0",
        "illuminate/database": "5.8.* | ^6.0 | ^7.0 | ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Adsy2010\\LaravelApiCredentials\\": "src"
        }
    }
}
